<?xml version="1.0" ?>
<launch>
    <arg name="real" default="false"/>
    <param name="real" value="$(arg real)"/>

    <arg name="start_way" default="manual"/>
    <param name="start_way" value="$(arg start_way)"/>

    <param name="joystick_sensitivity" value="20"/>
    
    <arg name="joystick_type" default="bt2"/>
    <param name="channel_map_path" value="$(find humanoid_controllers)/launch/joy/$(arg joystick_type).json"/>

    <include file="$(find joy)/launch/joy_node.launch">
    </include>
</launch>
