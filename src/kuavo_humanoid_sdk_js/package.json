{
    "name": "kuavo-humanoid-sdk",
    "version": "1.0.0",
    "description": "A JavaScript SDK for kuavo humanoid robot.",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "type": "module",
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "jest",
        "lint": "eslint src/**/*.{js,ts}",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build && npm test",
        "local-install": "npm pack && npm install -g ./kuavo-humanoid-sdk-*.tgz"
    },
    "keywords": [
        "kuavo",
        "humanoid",
        "robot",
        "robotics",
        "lejurobot",
        "ros",
        "sdk",
        "javascript",
        "typescript"
    ],
    "author": "lejurobot <edu@lejurobot.com>",
    "license": "MIT",
    "homepage": "https://gitee.com/leju-robot/kuavo-ros-opensource/",
    "repository": {
        "type": "git",
        "url": "https://gitee.com/leju-robot/kuavo-ros-opensource.git"
    },
    "bugs": {
        "url": "https://gitee.com/leju-robot/kuavo-ros-opensource/issues"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "eventemitter3": "^5.0.1",
        "ws": "^8.14.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-typescript": "^11.1.5",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "eslint": "^8.55.0",
        "jest": "^29.7.0",
        "rollup": "^4.46.3",
        "rollup-plugin-clean": "^1.0.0",
        "typescript": "^5.3.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./msg": {
            "import": "./dist/msg/index.esm.js",
            "require": "./dist/msg/index.js",
            "types": "./dist/msg/index.d.ts"
        },
        "./interfaces": {
            "import": "./dist/interfaces/index.esm.js",
            "require": "./dist/interfaces/index.js",
            "types": "./dist/interfaces/index.d.ts"
        },
        "./kuavo": {
            "import": "./dist/kuavo/index.esm.js",
            "require": "./dist/kuavo/index.js",
            "types": "./dist/kuavo/index.d.ts"
        },
        "./strategy": {
            "import": "./dist/kuavo_strategy/index.esm.js",
            "require": "./dist/kuavo_strategy/index.js",
            "types": "./dist/kuavo_strategy/index.d.ts"
        },
        "./package.json": "./package.json"
    }
}
